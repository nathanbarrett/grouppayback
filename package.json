{
  "name": "grouppayback",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck:app": "vue-tsc --noEmit -p tsconfig.app.json",
    "typecheck:workers": "tsc --noEmit -p tsconfig.workers.json",
    "deploy": "npm run build && wrangler deploy",
    "deploy:prod": "npm run build && wrangler deploy --env production"
  },
  "dependencies": {
    "simple-icons": "^16.6.0",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250115.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.23",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.17",
    "vue-tsc": "^3.1.4"
  }
}
